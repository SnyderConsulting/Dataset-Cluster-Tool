venv/
tf_env/
ENV/
env/
env.bak/
venv.bak/
.virtualenv/

# Jupyter Notebook checkpoints
.ipynb_checkpoints/

# Logs
*.log

# Distribution / packaging
build/
dist/
*.egg-info/
.venv/

# Installer logs
pip-log.txt
pip-delete-this-directory.txt

# Unit test / coverage reports
htmlcov/
.coverage
.tox/
.nox/
.cache/
.pytest_cache/
coverage.xml

# MyPy
.mypy_cache/

#-------------------------------------
# Node.js Ignore Patterns
#-------------------------------------

# Dependency directories
node_modules/
app/node_modules/
app/client/node_modules/

# Build outputs
build/
dist/
app/build/
app/dist/
app/client/build/
app/client/dist/

# Logs
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Runtime data
pids/
*.pid
*.seed
*.pid.lock

# Coverage directory
coverage/
app/coverage/
app/client/coverage/

# TypeScript cache
*.tsbuildinfo

# Diagnostic reports
report.[0-9]*.[0-9]*.[0-9]*.[0-9]*.json

#-------------------------------------
# Output and Generated Directories
#-------------------------------------

# Output directories (e.g., generated images)
app/output/
output/

